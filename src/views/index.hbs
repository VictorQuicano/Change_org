<div class="card mt-2">
  <div class="card-header bg-dark text-white">
    <h3><i class="far fa-images"></i> Nuevos proyectos</h3>
  </div>
  <div class="card-body">
    <div class="row">
      {{#each images}}
        <div class="d-flex align-items-center">
          <div class="col d-inline-block align-top">
            <a href="/images/{{uniqueId}}">
              <img src="/public/upload/{{filename}}" class="img-thumbnail" alt="{{title}}" width="300">
            </a>
          </div>
          <div class="col">
            <blockquote class="w-100">
              <h1 >{{title}}</h1>
                <footer class="blockquote-footer pt-3">
                  <span>{{description}}</span><br/><br/>
                <span><i class="fa fa-thumbs-up"></i>{{likes}}</span>
                </footer>
            </blockquote>
          </div>
        </div>
        <hr size="8px" color="black" />
      {{/each}}
    </div>
  </div>
</div>